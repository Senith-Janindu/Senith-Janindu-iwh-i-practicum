doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Object Table
        h3
            a(href='/homepage') Add/Update this table


            table(style='width:50%; ', )
                    tr
                        th.column1 Vehicle Name
                        th.column2 Vehicle Color
                        th.column3 Vehicle Model
                    each vehicle in data
                        tr
                            td.bordered #{vehicle.properties.vehicle_name}
                            td.bordered #{vehicle.properties.vehicle_color}
                            td.bordered #{vehicle.properties.vehicle_model}